void printTriangle(int n) {
	    // code here
	    
	    for(int i=n; i>0; i=i-1)
        {
            int k=1;
            for(int j=i; j>0; j=j-1)
            {
                cout<<k<<" ";
                k=k+1;
            }
            cout<<endl;
        }
	}

Input: 5

Output:
1 2 3 4 5
1 2 3 4
1 2 3 
1 2  
1 
